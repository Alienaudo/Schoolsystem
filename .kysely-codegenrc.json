{
    "camelCase": true,
    "dateParser": "timestamp",
    "defaultSchemas": [],
    "dialect": "postgres",
    "domains": true,
    "envFile": ".env",
    "excludePattern": null,
    "includePattern": null,
    "logLevel": "warn",
    "numericParser": "string",
    "outFile": "./src/database/kysely-types.d.ts",
    "overrides": {
        "columns": {
            "users.settings": "{ theme: 'dark' }"
        }
    },
    "partitions": false,
    "print": false,
    "runtimeEnums": false,
    "singularize": false,
    "typeOnlyImports": true,
    "url": "env(DATABASE_URL)",
    "verify": false
}
